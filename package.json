{
  "name": "foodreview",
  "version": "1.0.0",
  "description": "Food Review app for Montgomery, AL",
  "main": "index.js",
  "scripts": {
    "test": "testfood",
    "start": "cd backend && node --experimental-modules server.mjs && cd ../client && npm start",
    "start:server": "cd backend && nodemon server.js --exec babel-node --presets es2015,stage-0",
    "start:client": "cd client && npm start",
    "start:dev": "concurrently \"npm start:server\" \"npm start:client\""
